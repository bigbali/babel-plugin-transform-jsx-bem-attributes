{
    "name": "babel-plugin-transform-jsx-bem-attributes",
    "version": "0.1.1",
    "description": "A Babel plugin for transforming 'block', 'elem' and 'mods' attributes into a proper 'className'",
    "homepage": "https://github.com/bigbali/babel-plugin-transform-jsx-bem-attributes/",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bigbali/babel-plugin-transform-jsx-bem-attributes.git"
    },
    "files": [
        "lib/*.js",
        "index.d.ts"
    ],
    "keywords": [
        "babel",
        "react",
        "jsx",
        "bem",
        "props",
        "attributes",
        "transform"
    ],
    "main": "lib/index.js",
    "types": "./index.d.ts",
    "scripts": {
        "clear": "if exist lib rmdir /s /q lib",
        "build": "yarn clear && yarn run tsc --build",
        "test": "yarn build && yarn jest"
    },
    "author": "Balázs Burján",
    "license": "MIT",
    "dependencies": {
        "@babel/core": "^7.18.6",
        "@babel/preset-env": "^7.18.6",
        "@babel/preset-react": "^7.18.6",
        "@babel/types": "7.18.7"
    },
    "devDependencies": {
        "@types/babel__core": "^7.1.19",
        "@types/react": "^18.0.25",
        "@types/jest": "^29.0.3",
        "@types/node": "^18.7.18",
        "@typescript-eslint/eslint-plugin": "^5.38.0",
        "@typescript-eslint/parser": "^5.38.0",
        "eslint": "^8.23.1",
        "jest": "^29.0.3",
        "typescript": "^4.8.3"
    },
    "jest": {
        "testRegex": "/test/index.js"
    }
}